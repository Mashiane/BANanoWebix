#IgnoreWarnings:15
Sub Process_Globals
	Public BANano As BANano 'ignore
	Public AppName As String = "BANanoWebixApp"
	Private Publish As String
	Private BP As BANanoPostProcessor
	Public dbName As String
End Sub

Sub AppStart (Form1 As Form, Args() As String)
	Publish = "C:\xampp\htdocs"
	Publish = "C:\laragon\www"
	Dim Version As String = DateTime.now
	Dim appFile As String = $"app${Version}.js"$
	BANano.Initialize("BANano", AppName,1)
	BANano.HTML_NAME = "index.html"
	BANano.Header.Title = AppName
	BANano.JAVASCRIPT_NAME = appFile
	BANano.Header.addcssfile("all.min.css")
	BANano.Header.AddCSSFile("materialdesignicons.min.css")
	'
	BANano.Header.AddCSSFile("contrast.min.css")
	'*themes
	'BANano.Header.AddCSSFile("material.min.css")
	'BANano.Header.AddCSSFile("flat.min.css")
	'BANano.Header.AddCSSFile("compact.min.css")
	'BANano.Header.AddCSSFile("contrast.min.css")
	'** themes
	BANano.Header.AddJavascriptFile("webix.min.js")
	BANano.Header.AddCSSFile("hint.css")
	BANano.Header.AddJavascriptFile("hint.js")
	BANano.Header.AddJavascriptFile("skin.js")
	'
	BANano.Header.AddJavascriptFile("data.js")
	
	BANano.Header.AddJavascriptFile("Blob.min.js")
	BANano.Header.AddJavascriptFile("FileSaver.min.js")
	
	BANano.Header.AddJavascriptFile("https://togetherjs.com/togetherjs-min.js")
	
	'BANano.Header.AddCSSFile("pivot.min.css")
	'BANano.Header.AddJavascriptFile("pivot.min.js")
		
	'BANano.TranspilerOptions.ShowWarningDeadCode = True
	'BANano.TranspilerOptions.RemoveDeadCode = True
	BANano.TranspilerOptions.UseServiceWorker = False
	'BANano.TranspilerOptions.MergeAllCSSFiles = True
	'BANano.TranspilerOptions.MergeAllJavascriptFiles = True
	'php
	'set php settings
	BANano.PHP_NAME = "BANanoWebixApp.php"
	BANano.PHPHost = $"http://localhost/${AppName}/"$
	BANano.PHPAddHeader("Access-Control-Allow-Origin: *")
	'
	BANano.Build(Publish)
	'post processor
	BP.Initialize
	BP.PublishPath = Publish
	BP.ShortName = AppName
	BP.AssetsPath = BANano.ASSETS_FOLDER
	BP.ScriptsPath = BANano.SCRIPTS_FOLDER
	BP.StylesPath = BANano.STYLES_FOLDER
	BP.ScriptFile = BANano.JAVASCRIPT_NAME
	BP.UnzipFonts
	BP.UnzipFile("extras.zip")
	BP.Open(True)
	ExitApplication
End Sub

'Return true to allow the default exceptions handler to handle the uncaught exception.
Sub Application_Error (Error As Exception, StackTrace As String) As Boolean
	Return True
End Sub


Sub BANano_Ready()
	modSideBar.Version = "2.26"
	dbName = $"${AppName}.db"$
	Dim sqlite As BANanoSQLite
	sqlite.Initialize
	'create forms table
	Dim tbl As Map = CreateMap()
	tbl.Put("id", sqlite.DB_STRING)
	tbl.Put("value", sqlite.DB_STRING)
	tbl.Put("json", sqlite.DB_STRING)
	tbl.put("parentid", sqlite.DB_STRING)
	Dim qry As String = sqlite.CreateTable("forms", tbl, "id")
	Dim res As String = BANano.CallInlinePHPWait("BANanoSQLite", CreateMap("dbname": dbName, "data": qry))
	Dim rs As SQLiteResultSet = sqlite.GetResultSet(qry,res)
	'
	sqlite.Initialize
	Dim tbl As Map = CreateMap()
	tbl.Put("id", sqlite.DB_STRING)
	tbl.Put("key", sqlite.DB_STRING)
	tbl.Put("name", sqlite.DB_STRING)
	tbl.Put("json", sqlite.DB_STRING)
	tbl.put("parentid", sqlite.DB_STRING)
	tbl.put("tabindex", sqlite.DB_INT)
	Dim qry As String = sqlite.CreateTable("properties", tbl, "key")
	Dim res As String = BANano.CallInlinePHPWait("BANanoSQLite", CreateMap("dbname": dbName, "data": qry))
	Dim rs As SQLiteResultSet = sqlite.GetResultSet(qry,res)
	'
	sqlite.Initialize
	Dim tbl As Map = CreateMap()
	tbl.Put("name", sqlite.DB_STRING)
	tbl.Put("json", sqlite.DB_STRING)
	Dim qry As String = sqlite.CreateTable("wixsomething", tbl, "name")
	Dim res As String = BANano.CallInlinePHPWait("BANanoSQLite", CreateMap("dbname": dbName, "data": qry))
	Dim rs As SQLiteResultSet = sqlite.GetResultSet(qry,res)
	'
		
	sqlite.Initialize
