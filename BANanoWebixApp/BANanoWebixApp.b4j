	c.Put("id", sqlite.DB_STRING)
	c.Put("json", sqlite.DB_STRING)
	Dim qry As String = sqlite.CreateTable("connect", c, "id")
	Dim res As String = BANano.CallInlinePHPWait("BANanoSQLite", CreateMap("dbname": dbName, "data": qry))
	Dim rs As SQLiteResultSet = sqlite.GetResultSet(qry,res)
	'
	sqlite.Initialize
	Dim c As Map = CreateMap()
	c.Put("key", sqlite.DB_STRING)
